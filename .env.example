# Environment Configuration
NODE_ENV=development

# Database
DATABASE_URL=your_database_url_here

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Stripe Configuration
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key

# Feature Flags
ENABLE_PIX_PAYMENT=false
VITE_ENABLE_PIX_PAYMENT=false

# DocuSign Configuration
DOCUSIGN_CLIENT_ID=your_docusign_client_id
DOCUSIGN_USER_ID=your_docusign_user_id
DOCUSIGN_PRIVATE_KEY_PATH=./docusign_private.key

# Other
REPLIT_DOMAINS=your_domain.replit.app